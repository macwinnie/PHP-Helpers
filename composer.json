{
    "name": "macwinnie/phphelpers",
    "description": "reusable PHP functions, e.g. RegEx ones",
    "type": "library",
    "license": "CC-BY-SA-4.0",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "macwinnie",
            "email": "dev@macwinnie.me"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "behat/behat": ">=3.8",
        "phpunit/phpunit": ">=9.5"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "macwinnie\\PHPHelpers\\": "src/classes/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "macwinnie\\PHPHelpersTests\\" : "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/behat"
    }
}
